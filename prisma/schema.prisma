// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:rootroot@localhost:3306/mydb"
}

model Stock {
  id String @id @default(cuid())
  product String
  category String
  price Int
}
